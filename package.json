{
  "name": "digital-management-system-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "./script/env.sh && cp env-config.js ./public/ && react-scripts start",
    "start:windows": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command script/env.ps1;copy env-config.js .\\public\\ ; react-scripts start",
    "build": "sh -ac '. ./.env; react-scripts build'",
    "test": "react-scripts test --coverage",
    "test:ci": "CI=true react-scripts test",
    "test:ci:publish": "CI=true react-scripts test --coverage --coverageReporters=text-lcov | coveralls",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ./schema/schema.graphql --extensions js jsx",
    "relay:watch": "npm run relay -- --watch",
    "flow": "flow",
    "lint": "eslint src",
    "fixlint": "eslint --fix --ext .jsx --ext .js src",
    "format": "prettier --write --single-quote true --trailing-comma all --print-width 150 ./src && npm run fixlint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "clsx": "^1.1.0",
    "cuid": "^2.1.8",
    "email-validator": "^2.0.4",
    "firebase": "^7.14.2",
    "graphql-relay": "^0.6.0",
    "i18next": "^19.4.4",
    "immutable": "^4.0.0-rc.12",
    "node-sass": "^4.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-redux": "^7.2.0",
    "react-redux-firebase": "^3.4.0",
    "react-relay": "^9.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "recompose": "^0.30.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.3.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-relay": "^9.1.0",
    "chance": "^1.1.4",
    "coveralls": "^3.1.0",
    "flow-bin": "^0.123.0",
    "graphql": "^15.0.0",
    "husky": "^4.2.5",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "relay-compiler": "^9.1.0",
    "relay-config": "^9.1.0",
    "typescript": "^3.8.3"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write --single-quote true --trailing-comma all --print-width 150",
      "npm run fixlint",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "proxy": "http://localhost:5001/dms-morteza/us-central1/graphql",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
